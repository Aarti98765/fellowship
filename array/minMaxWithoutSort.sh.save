  randomNumberArray[$start]=$(( RANDOM%900+100 ));
done
for ((index=1; index<=10; index++))
do
  if [[ $index -eq 1 ]]
  then
	  minNumber=$


[randomNumber[$index];
	  maxNumber=$randomNumber[$index];
  elif [[ "${randomNumberArray[$index]}" -gt $maxNumber ]]	
